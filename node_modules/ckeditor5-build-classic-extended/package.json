{
  "name": "ckeditor5-build-classic-extended",
  "version": "20.0.1",
  "description": "Extended verison of the classic editor build of CKEditor 5",
  "keywords": [
    "ckeditor5-build",
    "ckeditor",
    "ckeditor5",
    "ckeditor 5",
    "wysiwyg",
    "rich text",
    "editor",
    "html",
    "contentEditable",
    "editing",
    "operational transformation",
    "ot",
    "collaboration",
